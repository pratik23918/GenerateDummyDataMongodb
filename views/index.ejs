    //sliders using bootstrap
    <div id="carouselExample" class="carousel slide">
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img src="https://wallpapers.com/images/featured/beautiful-scenery-wnxju2647uqrcccv.jpg" class="d-block w-100"width="23" height="433" alt="...">
